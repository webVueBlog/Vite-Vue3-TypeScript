{
  "name": "vite-vue3-typescript",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [
    "Vite",
    "Vue3",
    "TypeScript",
    "Starter"
  ],
  "bugs": {
    "url": "https://github.com/webVueBlog/Vite-Vue3-TypeScript/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webVueBlog/Vite-Vue3-TypeScript"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "element-plus": "^1.3.0-beta.5",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.2",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "stylus": "^0.56.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-element-plus": "^0.0.12",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.29.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
